{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"strict": true,
		"strictNullChecks": true,
		"esModuleInterop": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noUnusedLocals": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"jsx": "react-jsx",
		"declaration": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext"
	}
}
